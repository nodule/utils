{
  "name": "method",
  "ns": "utils",
  "description": "Execute a method",
  "phrases": {
    "active": "Executing method {{input.method}}"
  },
  "ports": {
    "input": {
      "in": {
        "type": "array",
        "title": "Arguments",
        "description": "arguments to be applied to this method",
        "default": []
      },
      "instance": {
        "type": "function",
        "title": "Instance",
        "description": "instance to invoke this method on"
      },
      "method": {
        "type": "string",
        "title": "Method name"
      }
    },
    "output": {
      "out": {
        "title": "Out",
        "type": "any"
      },
      "error": {
        "title": "Error",
        "type": "object"
      }
    }
  }
}
