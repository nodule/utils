{
  "name": "xml2js",
  "ns": "utils",
  "description": "Simple XML to JavaScript object converter",
  "phrases": {
    "active": "Converting XML to Object"
  },
  "ports": {
    "input": {
      "xml": {
        "type": "string",
        "format": "xml",
        "title": "XML"
      }
    },
    "output": {
      "error": {
        "type": "object",
        "title": "Error Object"
      },
      "result": {
        "type": "object"
      }
    }
  },
  "dependencies": {
    "npm": {
      "xml2js": "0.x.x"
    }
  }
}